            align-items: center;
            justify-content: center;
            font-size: 0.8em;
            color: #003594;
            font-weight: 800;
            flex-shrink: 0;
        }

        .summary-title {
            font-family: Georgia, "Times New Roman", "Songti SC", serif;
            font-size: 1.1em;
            font-weight: 700;
            letter-spacing: 0.03em;
            flex: 1;
        }

        .summary-badge {
            font-size: 0.6em;
            font-weight: 700;
            letter-spacing: 0.12em;
            text-transform: uppercase;
            color: #003594;
            background: #C4A962;
            padding: 4px 12px;
            border-radius: 3px;
        }

        .executive-summary .summary-body p {
            color: rgba(255,255,255,0.92);
            line-height: 1.78;
            margin-bottom: 14px;
            font-size: 0.95em;
        }

        .executive-summary .summary-body p:last-child { margin-bottom: 0; }

        .executive-summary .summary-body strong {
            color: #C4A962;
            font-weight: 700;
        }

        .executive-summary .summary-body code {
            font-family: "SF Mono", Monaco, "Courier New", monospace;
            background: rgba(255,255,255,0.12);
            padding: 1px 5px;
            border-radius: 3px;
            font-size: 0.88em;
        }

        .executive-summary .summary-body em {
            font-style: italic;
            color: rgba(255,255,255,0.8);
        }

        /* ============================================================
         * SECTION HEADER: Table-style
         * ============================================================ */
        .section {
            margin-bottom: 40px;
            page-break-inside: avoid;
        }

        .section-header-bar {
            background: linear-gradient(135deg, #003594 0%, #00256E 100%);
            color: #FFFFFF;
            display: flex;
            align-items: center;
            padding: 12px 20px;
