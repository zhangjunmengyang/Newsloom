            padding-bottom: 20px;
            border-bottom: 1px solid rgba(196, 169, 98, 0.35);
        }

        .header-report-title {
            font-family: Georgia, "Times New Roman", "Songti SC", "SimSun", serif;
            font-size: 1.35em;
            font-weight: 400;
            line-height: 1.35;
            margin-bottom: 20px;
            color: rgba(255,255,255,0.92);
        }

        .header-meta-grid {
            display: flex;
            gap: 32px;
            flex-wrap: wrap;
            font-size: 0.78em;
        }

        .header-meta-item {
            display: flex;
            flex-direction: column;
            gap: 2px;
        }

        .header-meta-label {
            font-size: 0.78em;
            font-weight: 700;
            letter-spacing: 0.1em;
            text-transform: uppercase;
            color: rgba(196, 169, 98, 0.8);
        }

        .header-meta-value {
            font-weight: 500;
            color: rgba(255,255,255,0.85);
        }

        /* --- Gold stripe after header --- */
        .header-bottom-stripe {
            height: 1px;
            background: linear-gradient(90deg, transparent, #C4A962, transparent);
            margin: 0 -24px;
        }

        /* ============================================================
         * METRICS PANEL: Data table style
         * ============================================================ */
        .metrics-panel {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            border: 1px solid #D1D5DB;
            margin: 32px 0;
            background: #FFFFFF;
        }

        .metric-cell {
            padding: 18px 12px;
            text-align: center;
            border-right: 1px solid #E5E7EB;
            position: relative;
        }

        .metric-cell:last-child { border-right: none; }

        .metric-cell:nth-child(odd) {
            background: #FAFBFD;
        }

        .metric-number {
            font-family: Georgia, "Times New Roman", serif;
            font-size: 1.8em;
            font-weight: 700;
            color: #003594;
            line-height: 1;
            margin-bottom: 6px;
        }

        .metric-label {
            font-size: 0.65em;
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            color: #737373;
        }

        /* ============================================================
