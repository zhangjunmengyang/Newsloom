                </div>
                <div class="header-meta-item">
                    <span class="header-meta-label">Coverage</span>
                    <span class="header-meta-value">{{ total_items }} Items</span>
                </div>
                <div class="header-meta-item">
                    <span class="header-meta-label">Sectors</span>
                    <span class="header-meta-value">{{ section_order|length }} Active</span>
                </div>
            </div>
        </div>
    </div>
    <div class="header-bottom-stripe"></div>

    <div class="container">

        <!-- ====== KEY METRICS ====== -->
        {% set ns = namespace(must_count=0, rec_count=0, fyi_count=0) %}
        {% for section_key, items in briefs.items() %}
        {% if items is iterable and items is not string %}
        {% for item in items %}
        {% if item.priority == 'ðŸ”´' %}{% set ns.must_count = ns.must_count + 1 %}
        {% elif item.priority == 'ðŸŸ¡' %}{% set ns.rec_count = ns.rec_count + 1 %}
        {% else %}{% set ns.fyi_count = ns.fyi_count + 1 %}
        {% endif %}
        {% endfor %}
        {% endif %}
        {% endfor %}

        <div class="metrics-panel">
            <div class="metric-cell">
                <div class="metric-number">{{ total_items }}</div>
                <div class="metric-label">Total Coverage</div>
            </div>
            <div class="metric-cell">
                <div class="metric-number" style="color: #C4A962;">{{ ns.must_count }}</div>
                <div class="metric-label">Conviction List</div>
            </div>
            <div class="metric-cell">
                <div class="metric-number">{{ ns.rec_count }}</div>
                <div class="metric-label">Recommended</div>
            </div>
            <div class="metric-cell">
                <div class="metric-number">{{ section_order|length }}</div>
                <div class="metric-label">Sectors</div>
            </div>
        </div>

        <!-- ====== EXECUTIVE SUMMARY: Bottom Line Box ====== -->
        {% if executive_summary %}
        <div class="executive-summary">
            <div class="summary-label">
                <div class="summary-icon">BL</div>
                <div class="summary-title">The Bottom Line</div>
                <span class="summary-badge">Key Insights</span>
            </div>
            <div class="summary-body">
                {% for line in executive_summary.strip().split('\n') %}
                {% if line.strip() %}
                <p>{{ line.strip() | md_inline }}</p>
                {% endif %}
                {% endfor %}
            </div>
        </div>
        {% endif %}

        <!-- ====== SECTIONS ====== -->
        {% set ns_sec = namespace(num=1) %}
        {% for section_key in section_order %}
        {% if section_key in briefs and briefs[section_key] is iterable and briefs[section_key] is not string and briefs[section_key]|length > 0 %}
        {% set meta = section_configs.get(section_key, {}) %}
        {% set items = briefs[section_key] %}

        {% if ns_sec.num > 1 %}
        <div class="section-divider">
            <div class="section-divider-line"></div>
            <div class="section-divider-diamond"></div>
            <div class="section-divider-line"></div>
        </div>
        {% endif %}

        <section class="section">
            <!-- Section Header: Deep blue bar -->
            <div class="section-header-bar">
                <span class="section-emoji">{{ meta.emoji|default('ðŸ“Š') }}</span>
                <h2 class="section-title">{{ meta.title|default(section_key) }}</h2>
                <span class="section-color-indicator" style="background: {{ meta.color|default('#003594') }};"></span>
                <span class="section-count-badge">{{ items|length }} Items</span>
            </div>

            <!-- Table Header -->
            <div class="data-table-header">
                <div style="text-align: center;">Pr.</div>
                <div>Research Item</div>
                <div style="text-align: center;">Source</div>
            </div>

            <!-- Brief Rows -->
            <div class="data-table">
                {% for item in items %}
                {% set priority = item.priority|default('ðŸŸ¢') %}
                {% set p_class = 'conviction' if priority == 'ðŸ”´' else ('recommended' if priority == 'ðŸŸ¡' else 'fyi') %}
                <div class="brief-row {{ p_class }}">
                    {% if priority == 'ðŸ”´' %}
                    <div><span class="conviction-badge">â˜… Conviction List</span></div>
                    {% endif %}

                    <div class="brief-top-row">
                        <span class="brief-priority-dot">{{ priority }}</span>
                        <h3 class="brief-headline">
                            <a href="{{ item.url|default('#') }}" target="_blank">{{ item.headline|default('Untitled') }}</a>
                        </h3>
                    </div>

                    {% if item.source or item.tags %}
                    <div class="brief-meta-row">
                        {% if item.source %}
                        <span class="source-chip">{{ item.source }}</span>
                        {% endif %}
                        {% if item.tags %}
                        {% for tag in item.tags %}
                        <span class="tag-chip">{{ tag }}</span>
                        {% endfor %}
                        {% endif %}
                    </div>
                    {% endif %}

                    {% if item.detail %}
                    <div class="brief-detail">{{ item.detail | md_inline }}</div>
                    {% endif %}
                </div>
                {% endfor %}
            </div>

            <!-- Bottom Line Box for each section -->
            {% set ns_bl = namespace(must_items=0) %}
            {% for item in items %}
            {% if item.priority == 'ðŸ”´' %}{% set ns_bl.must_items = ns_bl.must_items + 1 %}{% endif %}
            {% endfor %}
            {% if ns_bl.must_items > 0 %}
            <div class="bottom-line-box">
                <strong>{{ meta.title|default(section_key) }}</strong> sector: {{ items|length }} items tracked, <strong>{{ ns_bl.must_items }}</strong> on our Conviction List requiring immediate attention. {{ meta.emoji|default('') }}
            </div>
            {% endif %}
        </section>

        {% set ns_sec.num = ns_sec.num + 1 %}
        {% endif %}
        {% endfor %}

        <!-- ====== ANALYST ATTRIBUTION ====== -->
        <div class="analyst-box">
            <div class="analyst-avatar">NL</div>
            <div class="analyst-info">
                <div class="analyst-name">Newsloom Intelligence Engine</div>
                <div class="analyst-title-text">Automated Research Division Â· Multi-Sector Coverage</div>
                <div class="analyst-contact">newsloom-research@gs-intelligence.internal</div>
            </div>
            <div class="analyst-stamp">
                <div class="analyst-stamp-date">{{ date_str }}</div>
                <div class="analyst-stamp-time">{{ generated_time }}</div>
            </div>
        </div>

        <!-- ====== FOOTER ====== -->
        <footer class="footer">
            <div class="footer-legal">
                <div class="footer-legal-title">Important Disclosures</div>
                This research report is prepared for informational purposes only and does not constitute an offer, solicitation, or recommendation to buy or sell any securities. The information contained herein has been obtained from sources believed to be reliable, but Goldman Sachs makes no representation or warranty as to its accuracy or completeness. Past performance is not indicative of future results. This document is intended for authorized recipients only and may not be reproduced, redistributed, or passed to any other person or published in whole or in part for any purpose without the prior written consent of Goldman Sachs. Goldman Sachs and its affiliates may have positions in the securities mentioned in this report.
            </div>
            <div class="footer-brand-bar">
                <span class="footer-brand-name">Goldman Sachs</span>
                <span class="footer-powered">Powered by Newsloom Intelligence</span>
                <span class="footer-copyright">Â© {{ date_str[:4] }} The Goldman Sachs Group, Inc. All Rights Reserved.</span>
            </div>
        </footer>

    </div>
</body>
</html>