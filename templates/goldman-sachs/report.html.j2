        }

        .section-title {
            font-family: Georgia, "Times New Roman", "Songti SC", serif;
            font-size: 1.05em;
            font-weight: 700;
            letter-spacing: 0.03em;
            flex: 1;
        }

        .section-color-indicator {
            width: 10px;
            height: 10px;
            border-radius: 50%;
            border: 2px solid rgba(255,255,255,0.4);
            flex-shrink: 0;
        }

        .section-count-badge {
            font-size: 0.68em;
            font-weight: 700;
            letter-spacing: 0.06em;
            color: #003594;
            background: #C4A962;
            padding: 3px 12px;
            border-radius: 2px;
            text-transform: uppercase;
        }

        /* ============================================================
         * DATA TABLE: Striped rows, professional grid
         * ============================================================ */
        .data-table {
            width: 100%;
            border: 1px solid #D1D5DB;
            border-top: none;
            border-collapse: collapse;
        }

        .data-table-header {
            display: grid;
            grid-template-columns: 40px 1fr 100px;
            background: #F3F4F6;
            border-bottom: 2px solid #D1D5DB;
            font-size: 0.68em;
            font-weight: 700;
            text-transform: uppercase;
            letter-spacing: 0.08em;
            color: #4A4A4A;
        }

        .data-table-header > div {
            padding: 8px 12px;
            border-right: 1px solid #E5E7EB;
        }

        .data-table-header > div:last-child { border-right: none; }

        /* --- Brief Row --- */
        .brief-row {
            border-bottom: 1px solid #F0F1F3;
            padding: 18px 20px;
            transition: background 0.15s;
            position: relative;
            page-break-inside: avoid;
        }

        .brief-row:hover { background: #F5F7FA; }

        /* Striped rows */
        .brief-row:nth-child(even) {
            background: #FAFBFD;
        }
        .brief-row:nth-child(even):hover { background: #F0F2F5; }

        /* Must-read: Conviction List */
        .brief-row.conviction {
            background: linear-gradient(90deg,
                rgba(196, 169, 98, 0.08) 0%,
                rgba(196, 169, 98, 0.04) 30%,
                transparent 70%);
            border-left: 4px solid #C4A962;
        }
        .brief-row.conviction:nth-child(even) {
            background: linear-gradient(90deg,
                rgba(196, 169, 98, 0.10) 0%,
                rgba(196, 169, 98, 0.05) 30%,
                #FAFBFD 70%);
        }

        .brief-row.recommended {
            border-left: 3px solid #003594;
        }

        .brief-row.fyi {
            opacity: 0.88;
            border-left: 3px solid #D1D5DB;
        }

        /* Conviction List Badge */
        .conviction-badge {
            display: inline-block;
            font-size: 0.58em;
            font-weight: 800;
            letter-spacing: 0.14em;
            text-transform: uppercase;
            color: #FFFFFF;
            background: linear-gradient(135deg, #C4A962, #B89A52);
            padding: 3px 10px;
            border-radius: 2px;
            margin-bottom: 8px;
            box-shadow: 0 1px 3px rgba(196, 169, 98, 0.3);
        }

        .brief-top-row {
            display: flex;
            align-items: flex-start;
            gap: 10px;
            margin-bottom: 8px;
        }

        .brief-priority-dot {
            flex-shrink: 0;
            font-size: 0.92em;
            margin-top: 3px;
        }

        .brief-headline {
            font-family: Georgia, "Times New Roman", "Songti SC", serif;
            font-size: 1.02em;
            font-weight: 600;
            line-height: 1.42;
            flex: 1;
        }

        .brief-headline a { color: #1A1A1A; }
        .brief-headline a:hover { color: #003594; }

        .brief-row.conviction .brief-headline a {
            color: #003594;
            font-weight: 700;
        }

        /* Meta row */
        .brief-meta-row {
            display: flex;
            align-items: center;
            gap: 8px;
            margin-bottom: 8px;
            flex-wrap: wrap;
        }

        .source-chip {
            font-size: 0.7em;
            font-weight: 700;
            color: #003594;
            background: rgba(0, 53, 148, 0.06);
            border: 1px solid rgba(0, 53, 148, 0.12);
            padding: 2px 8px;
            border-radius: 2px;
            text-transform: uppercase;
            letter-spacing: 0.04em;
        }

        .tag-chip {
            font-size: 0.65em;
            font-weight: 500;
            color: #737373;
            background: #F3F4F6;
            border: 1px solid #E5E7EB;
            padding: 2px 7px;
            border-radius: 2px;
        }

        .brief-detail {
            color: #4A4A4A;
            font-size: 0.92em;
            line-height: 1.72;
            text-align: justify;
            padding-left: 26px;
        }

        .brief-detail strong {
            color: #1A1A1A;
            font-weight: 600;
        }

        .brief-detail em {
            font-style: italic;
            color: #4A4A4A;
        }

        .brief-detail code {
            font-family: "SF Mono", Monaco, "Courier New", monospace;
            background: #F3F4F6;
            border: 1px solid #E5E7EB;
            padding: 1px 5px;
            border-radius: 2px;
            font-size: 0.88em;
            color: #1A1A1A;
        }

        /* ============================================================
         * BOTTOM LINE BOX: Per-section summary (blue bg white text)
         * ============================================================ */
        .bottom-line-box {
            background: linear-gradient(135deg, #003594 0%, #001A4D 100%);
            color: #FFFFFF;
            padding: 18px 24px;
            margin-top: 2px;
            border-bottom-left-radius: 4px;
            border-bottom-right-radius: 4px;
            font-size: 0.85em;
            line-height: 1.65;
            position: relative;
        }

        .bottom-line-box::before {
            content: 'BOTTOM LINE';
            font-size: 0.65em;
            font-weight: 800;
            letter-spacing: 0.15em;
            color: #C4A962;
            display: block;
            margin-bottom: 6px;
        }

        .bottom-line-box strong { color: #C4A962; }

        /* ============================================================
         * SECTION DIVIDER: Gold pinstripe
         * ============================================================ */
        .section-divider {
            display: flex;
            align-items: center;
            gap: 16px;
            margin: 8px 0 40px;
        }

        .section-divider-line {
            flex: 1;
            height: 1px;
            background: linear-gradient(90deg, transparent, #C4A962, transparent);
        }

        .section-divider-diamond {
            width: 8px;
            height: 8px;
            background: #C4A962;
            transform: rotate(45deg);
            flex-shrink: 0;
        }

        /* ============================================================
         * ANALYST ATTRIBUTION
         * ============================================================ */
        .analyst-box {
            display: flex;
            align-items: center;
            gap: 20px;
            background: #FAFBFD;
            border: 1px solid #E5E7EB;
            padding: 20px 24px;
            margin: 40px 0;
            page-break-inside: avoid;
        }

        .analyst-avatar {
            width: 48px;
            height: 48px;
            border-radius: 50%;
            background: linear-gradient(135deg, #003594 0%, #001A4D 100%);
            display: flex;
            align-items: center;
            justify-content: center;
            color: #C4A962;
            font-family: Georgia, serif;
            font-size: 1.2em;
            font-weight: 700;
            flex-shrink: 0;
        }

        .analyst-info {
            flex: 1;
        }

        .analyst-name {
            font-family: Georgia, "Times New Roman", serif;
            font-size: 1em;
            font-weight: 700;
            color: #1A1A1A;
            margin-bottom: 2px;
        }

        .analyst-title-text {
            font-size: 0.78em;
            color: #737373;
            margin-bottom: 2px;
        }

        .analyst-contact {
            font-size: 0.72em;
            color: #9CA3AF;
        }

        .analyst-stamp {
            text-align: right;
        }

        .analyst-stamp-date {
            font-family: Georgia, serif;
            font-size: 0.78em;
            color: #003594;
            font-weight: 600;
        }

        .analyst-stamp-time {
            font-size: 0.68em;
            color: #9CA3AF;
        }

        /* ============================================================
         * FOOTER: Regulatory disclaimer + copyright
         * ============================================================ */
        .footer {
            margin-top: 48px;
            border-top: 3px solid #003594;
            position: relative;
        }

        .footer::before {
            content: '';
            position: absolute;
            top: 3px; left: 0; right: 0;
            height: 2px;
            background: #C4A962;
        }

        .footer-legal {
            background: #FAFBFD;
            border: 1px solid #E5E7EB;
            border-top: none;
            padding: 24px;
            font-size: 0.68em;
            color: #9CA3AF;
            line-height: 1.6;
            text-align: justify;
        }

        .footer-legal-title {
            font-weight: 800;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            color: #737373;
            margin-bottom: 8px;
            font-size: 0.92em;
        }

        .footer-brand-bar {
            background: linear-gradient(135deg, #003594 0%, #001A4D 100%);
            color: #FFFFFF;
            text-align: center;
            padding: 16px 24px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .footer-brand-name {
            font-family: Georgia, "Times New Roman", serif;
            font-size: 0.88em;
            font-weight: 700;
            letter-spacing: 0.04em;
        }

        .footer-copyright {
            font-size: 0.68em;
            color: rgba(255,255,255,0.5);
        }

        .footer-powered {
            font-size: 0.62em;
            color: #C4A962;
            letter-spacing: 0.08em;
        }

        /* ============================================================
         * PRINT STYLES
         * ============================================================ */
        @media print {
            body {
                background: #FFFFFF !important;
                font-size: 11pt;
                -webkit-print-color-adjust: exact !important;
                print-color-adjust: exact !important;
            }

            .container {
                max-width: 100%;
                padding: 0;
            }

            .disclaimer-banner,
            .gold-stripe,
            .header-bottom-stripe {
                -webkit-print-color-adjust: exact;
            }

            .report-header {
                margin: 0;
                page-break-after: avoid;
            }

            .section {
                page-break-inside: avoid;
            }

            .brief-row {
                break-inside: avoid;
                page-break-inside: avoid;
            }

            .brief-row:hover { background: transparent !important; }
            .brief-row:nth-child(even):hover { background: #FAFBFD !important; }

            .executive-summary {
                page-break-inside: avoid;
            }

            .bottom-line-box {
                page-break-inside: avoid;
            }

            a { color: #1A1A1A !important; }
        }

        /* ============================================================
         * RESPONSIVE
         * ============================================================ */
        @media (max-width: 768px) {
            .container { padding: 0 16px 32px; }

            .report-header {
                padding: 32px 20px 28px;
                margin: 0 -16px 0;
            }

            .header-brand { font-size: 1.6em; }

            .header-meta-grid {
                gap: 16px;
            }

            .metrics-panel {
                grid-template-columns: repeat(2, 1fr);
            }

            .metric-cell:nth-child(2) { border-right: none; }

            .section-header-bar {
                padding: 10px 14px;
            }

            .brief-row { padding: 14px 14px; }
            .brief-detail { padding-left: 0; }

            .analyst-box {
                flex-direction: column;
                text-align: center;
                gap: 12px;
            }

            .analyst-stamp { text-align: center; }

            .footer-brand-bar {
                flex-direction: column;
                gap: 6px;
            }
        }

        @media (max-width: 480px) {
            body { font-size: 13px; }

            .report-header { padding: 24px 16px 20px; }
            .header-brand { font-size: 1.35em; }
            .header-report-title { font-size: 1.1em; }

            .metrics-panel { grid-template-columns: 1fr 1fr; }

            .executive-summary { padding: 20px; }

            .brief-headline { font-size: 0.95em; }
        }
    </style>
</head>
<body>

    <!-- ====== DISCLAIMER BANNER ====== -->
